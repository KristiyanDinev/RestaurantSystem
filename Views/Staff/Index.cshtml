@model RestaurantSystem.Models.DatabaseModels.UserModel

@{
    ViewData["Title"] = "Staff";
}


<head>
    @await Html.PartialAsync("_Imports")
</head>

<body>

    <p>Logged in as: @Model.Name</p>


    @if (Model.Image != null)
    {
        <img hight="500" width="500" src="~@Model.Image">
    }

    @await Html.PartialAsync("_RestaurantBar", Model.Restaurant)

    <h1>Home Staff Page</h1>

</body>
